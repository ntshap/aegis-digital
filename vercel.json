{
  "buildCommand": "cd aegis-digital-frontend/my-app && npm run build",
  "outputDirectory": "aegis-digital-frontend/my-app/.next",
  "installCommand": "cd aegis-digital-frontend/my-app && npm install",
  "devCommand": "cd aegis-digital-frontend/my-app && npm run dev",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "aegis-digital-frontend/my-app/src/app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
